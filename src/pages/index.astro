---
import Layout from '../layouts/Layout.astro';
import Card from '../components/Card.astro'; 

---

<Layout title="Music">
		<header class="relative w-auto h-full m-0">
			<div class="absolute  w-full p-0 m-0 top-[-45px]  container_intro h-[1200px] overflow-hidden">
